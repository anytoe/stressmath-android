<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mathchallenge.anytoe.com.mathchallenge"
    android:versionCode="4"
    android:versionName="1.3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme">
        <activity
            android:name=".controller.start.StartActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.game.GameActivity"
            android:label="@string/title_activity_game"></activity>
        <activity
            android:name=".controller.highscore.HighscoreActivity"
            android:label="@string/title_activity_highscore"></activity>
    </application>

</manifest>
